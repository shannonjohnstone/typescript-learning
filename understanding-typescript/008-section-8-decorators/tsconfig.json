{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2017",
      "dom.iterable",
      "scripthost"
    ],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules"
  ],
}